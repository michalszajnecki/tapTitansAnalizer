{
    "name": "TapTitans: Analizer",
    "version": "1.0",
    "description": "New tap titans Analizer",
    "devtools_page": "devtools.html",
    "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
    "permissions": [
      "unlimitedStorage",
      "storage",
      "webRequest",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "manifest_version": 2
}
